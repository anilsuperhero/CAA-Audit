<% include ../../partials/examplesHead %>


<% include ../../partials/docBody %>


<form class="eg" action="" method="post" data-busy="form" id="myForm" sty;>
  <div class="form-group">
    <label for="signerEmail">Signer Email</label>
    <input type="email" class="form-control" id="signerEmail" name="signerEmail"
          aria-describedby="emailHelp"  required
          value="<%= data.email %>">
    <small id="emailHelp" class="form-text text-muted">We'll never share your email with anyone else.</small>
  </div>
  <div class="form-group">
    <label for="signerName">Signer Name</label>
    <input type="text" class="form-control" id="signerName"  name="signerName" value="<%= data.name %>" required>
    <input type="hidden" class="form-control" id="fileName"  name="fileName" value="<%= data.file %>" required>
    <input type="hidden" class="form-control" id="auditId"  name="auditId" value="<%= data.id %>" required>
  </div>
  <input type="hidden" name="_csrf" value="<%- csrfToken %>">
  <button type="submit" class="btn btn-docu">Submit</button>
</form>
<% include ../../partials/examplesFoot.ejs %>

<script>
$(document).ready(function(){
	setTimeout(function(){
	  $("#myForm").submit()
	}, 1000);
});
</script>
